<div class="modal fade" id="modalPerfil" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLabel">Modal title</h5>
          <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
        </div>
        <div class="modal-body">


          <div class="container">
            <div class="row">
              <div class="col-lg-4 col-md-4 col-sm-12">
                  <img height="200em" src="imagen/<%=email%>" alt="">
              </div>
              <div class="col-lg-8 col-md-8 col-sm-12">
              
                  <h1><%=nombre%></h1>
                  <p class="mb-0"><strong>Fecha de creación:</strong><%=fecha%></p>
                  <p class="mb-0"><strong>Perfil: </strong><%=perfil%></p>
                  <p class="mb-0"><strong>Contraseña: </strong>*********************</p>
                  <p class="mb-0"><strong>Nombre para mostrar: </strong> <%=nombre%></p>  
                
              </div>
            </div>
            <div class="row mt-3">
                <div class="col-lg-12 col-md-12 col-sm-12">
                  <p class="mb-0">Avisos realizados:</p>
                  <p class="mb-0">Incidencias:</p>
                  <p class="mb-0">Sugerencias:</p>
                  <p class="mb-0">Felicitaciones:</p> 
                </div>
            </div>
          </div>



        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
          <button type="button" class="btn btn-primary">Save changes</button>
        </div>
      </div>
    </div>
  </div>